{
  "name": "euroweb-platform",
  "version": "8.0.0",
  "description": "EuroWeb - Pure TypeScript Industrial Web8 Platform",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --port 3000",
    "dev:backend": "tsx backend/server.ts",
    "dev:full": "concurrently \"yarn dev\" \"yarn dev:backend\"",
    "build": "yarn sw:build && next build",
    "start": "next start",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:vitest": "vitest run",
    "test:vitest:watch": "vitest",
    "type-check": "tsc --noEmit",
    "tech:guard": "tsx scripts/tech-guard.ts",
    "pure:check": "tsx scripts/pure-runner.ts check",
    "pure:build": "tsx scripts/pure-runner.ts build",
    "pure:test": "tsx scripts/pure-runner.ts test",
    "pure:dev": "tsx scripts/pure-runner.ts dev",
    "fix:tests": "tsx scripts/test-fixer.ts",
    "fix:routes": "tsx scripts/check-routes.ts --fix",
    "check:purity": "tsx scripts/pure-runner.ts check components/AGISheet/AGIxBioNature-pure.tsx",
    "industrial:build": "yarn fix:tests && yarn pure:build",
    "industrial:test": "yarn fix:tests && yarn test",
    "project:init": "powershell -ExecutionPolicy Bypass -File scripts/project-manager.ps1 init",
    "project:validate": "powershell -ExecutionPolicy Bypass -File scripts/project-manager.ps1 validate",
    "project:purity": "powershell -ExecutionPolicy Bypass -File scripts/project-manager.ps1 purity",
    "project:protect": "powershell -ExecutionPolicy Bypass -File scripts/project-manager.ps1 protect",
    "project:full": "powershell -ExecutionPolicy Bypass -File scripts/project-manager.ps1 full",
    "project:fix-paths": "powershell -ExecutionPolicy Bypass -File scripts/project-manager.ps1 fix-paths",
    "css:check": "tsx scripts/css-check.ts",
    "css:verify": "tsx scripts/css-check.ts",
    "clean": "rimraf dist .next coverage",
    "clean:all": "yarn clean && rimraf node_modules && yarn install",
    "sw:build": "tsx scripts/build-sw.ts build",
    "sw:watch": "tsx scripts/build-sw.ts watch",
    "sw:dev": "tsx scripts/build-sw.ts dev",
    "deploy": "powershell -ExecutionPolicy Bypass -File scripts/deploy.ps1",
    "deploy:vercel": "powershell -ExecutionPolicy Bypass -File scripts/deploy.ps1 -Target vercel",
    "deploy:docker": "powershell -ExecutionPolicy Bypass -File scripts/deploy.ps1 -Target docker",
    "deploy:compose": "powershell -ExecutionPolicy Bypass -File scripts/deploy.ps1 -Target docker-compose",
    "deploy:git": "powershell -ExecutionPolicy Bypass -File scripts/deploy.ps1 -Target git",
    "docker:build": "docker build -t euroweb-platform:latest .",
    "docker:run": "docker run -d --name euroweb-platform -p 3000:3000 euroweb-platform:latest",
    "docker:stop": "docker stop euroweb-platform && docker rm euroweb-platform",
    "docker:logs": "docker logs euroweb-platform -f",
    "compose:up": "docker-compose up -d",
    "compose:down": "docker-compose down",
    "compose:logs": "docker-compose logs -f",
    "setup:stack": "tsx scripts/setup-stack.ts"
  },
  "dependencies": {
    "@huggingface/inference": "4.5.3",
    "@popperjs/core": "2.11.8",
    "@react-pdf/renderer": "4.3.0",
    "@tanstack/react-table": "8.21.3",
    "@tiptap/core": "3.0.7",
    "@tiptap/extension-bold": "3.0.7",
    "@tiptap/extension-document": "3.0.7",
    "@tiptap/extension-italic": "3.0.7",
    "@tiptap/extension-paragraph": "3.0.7",
    "@tiptap/extension-underline": "3.0.7",
    "@tiptap/react": "3.0.7",
    "@visx/network": "3.12.0",
    "@xenova/transformers": "2.17.2",
    "chart.js": "4.5.0",
    "class-variance-authority": "^0.7.1",
    "colors": "^1.4.0",
    "compromise": "14.14.4",
    "cors": "^2.8.5",
    "crypto-js": "4.2.0",
    "d3": "7.9.0",
    "dotenv": "^17.2.0",
    "eslint": "^9.31.0",
    "eventemitter3": "^5.0.1",
    "excel-formula": "1.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "framer-motion": "^11.5.0",
    "franc-min": "6.2.0",
    "handsontable": "16.0.1",
    "helmet": "^8.1.0",
    "hot-formula-parser": "4.0.0",
    "jose": "6.0.12",
    "langchain": "0.3.30",
    "lucide-react": "^0.525.0",
    "mammoth": "1.9.1",
    "next": "^14.2.32",
    "papaparse": "5.5.3",
    "pdfjs-dist": "5.3.93",
    "react": "^18.3.1",
    "react-audio-player": "0.17.0",
    "react-chartjs-2": "5.3.0",
    "react-dom": "^18.3.1",
    "react-image-gallery": "1.4.0",
    "react-intl": "7.1.11",
    "react-virtualized": "9.22.6",
    "react-window": "1.8.11",
    "rxjs": "^7.8.2",
    "sentiment": "5.0.2",
    "socket.io": "^4.8.1",
    "typescript": "^5.8.3",
    "valtio": "^2.1.5",
    "winston": "^3.17.0",
    "xlsx": "0.18.5",
    "y-websocket": "3.0.0",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/cors": "^2.8.19",
    "@types/crypto-js": "^4",
    "@types/d3": "^7",
    "@types/eventemitter3": "^2.0.4",
    "@types/express": "^5.0.3",
    "@types/glob": "9.0.0",
    "@types/helmet": "^4.0.0",
    "@types/hot-formula-parser": "^4",
    "@types/identity-obj-proxy": "^3",
    "@types/node": "^22.15.18",
    "@types/papaparse": "^5",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "@types/react-image-gallery": "^1",
    "@types/react-virtualized": "^9",
    "@types/react-window": "^1",
    "@types/sentiment": "^5",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitest/ui": "^3.2.4",
    "chokidar": "^4.0.3",
    "concurrently": "^9.2.0",
    "eslint-config-next": "^15.4.2",
    "glob": "11.0.3",
    "identity-obj-proxy": "3.0.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.1.0",
    "rimraf": "^6.0.1",
    "ts-node": "10.9.2",
    "tsx": "^4.19.0",
    "typescript": "^5.7.2",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=4.9.2"
  },
  "author": {
    "name": "Ledjan Ahmati",
    "email": "dealsjona@gmail.com"
  },
  "license": "MIT",
  "packageManager": "yarn@4.9.2"
}
