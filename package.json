{
  "name": "euroweb-platform",
  "version": "8.0.0",
  "description": "EuroWeb - Pure TypeScript Industrial Web8 Platform",
  "private": true,
  "type": "module",
  "scripts": {
    "dupes": "node --loader ts-node/esm tools/find-duplicates.ts --root ./ --out reports/dupes --threshold=0.90",
    "dupes:ci": "npm run dupes && node --loader ts-node/esm tools/fail-on-duplicates.ts --dup=reports/dupes/duplicates.json --near=reports/dupes/near-duplicates.json --maxExact=0 --maxNear=0",
    "dev": "next dev",
    "dev:backend": "tsx backend/server.ts",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:backend\"",
    "dev:swiss": "yarn dev:full",
    "build": "next build",
    "build:swiss": "yarn type-check && yarn build",
    "start": "next start",
    "start:swiss": "yarn build:swiss && yarn start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:system": "tsx tests/euroWebUltra.system.test.ts",
    "test:all": "npm run test && npm run test:system",
    "test:api": "tsx tests/api-real-test.ts",
    "test:heavy": "tsx tests/real-heavy-test.ts",
    "test:stress": "yarn test:heavy",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:lint": "npm run format && npm run lint",
    "pre-commit": "lint-staged",
    "tech:guard": "tsx scripts/tech-guard.ts",
    "clean": "rimraf dist .next coverage",
    "clean:all": "npm run clean && rimraf node_modules && yarn install",
    "sw:build": "tsx scripts/build-sw.ts build",
    "sw:watch": "tsx scripts/build-sw.ts watch",
    "sw:dev": "tsx scripts/build-sw.ts dev",
    "backup:create": "tsx scripts/backup.ts create",
    "backup:list": "tsx scripts/backup.ts list",
    "backup:clean": "tsx scripts/backup.ts clean",
    "security:audit": "npm audit && yarn audit",
    "version:check": "type VERSION.txt",
    "swiss:precision": "yarn type-check && yarn lint && yarn test:api && yarn test:heavy",
    "swiss:deploy": "yarn swiss:precision && yarn build:swiss",
    "swiss:stress": "yarn test:heavy"
  },
