{
  "name": "euroweb-platform",
  "version": "8.0.0",
  "description": "EuroWeb - Pure TypeScript Industrial Web8 Platform",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --port 3000",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "dev:backend": "tsx backend/src/server.ts",
    "dev:realtime": "tsx backend/src/realtime-server.ts",
    "dev:simple": "tsx backend/src/simple-realtime-server.ts",
    "dev:lora": "tsx backend/lora-gateway/server.ts",
    "dev:full": "concurrently \"yarn dev\" \"yarn dev:backend\" \"yarn dev:realtime\" \"yarn dev:lora\"",
    "dev:hybrid": "tsx lib/hybrid/run-hybrid.ts",
    "dev:fluid": "tsx lib/hybrid/run-hybrid.ts",
    "build": "yarn sw:build && next build --no-lint",
    "build:with-lint": "next lint && yarn sw:build && next build",
    "start": "next start",
    "start:hybrid": "NODE_ENV=production tsx lib/hybrid/run-hybrid.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:vitest": "vitest run",
    "test:vitest:watch": "vitest",
    "type-check": "tsc --noEmit",
    "tech:guard": "tsx scripts/tech-guard.ts",
    "pure:check": "tsx scripts/pure-runner.ts check",
    "pure:build": "tsx scripts/pure-runner.ts build",
    "pure:test": "tsx scripts/pure-runner.ts test",
    "pure:dev": "tsx scripts/pure-runner.ts dev",
    "fix:tests": "tsx scripts/test-fixer.ts",
    "fix:routes": "tsx scripts/check-routes.ts --fix",
    "check:purity": "tsx scripts/pure-runner.ts check components/AGISheet/AGIBioNature-pure.tsx",
    "industrial:build": "yarn fix:tests && yarn pure:build",
    "industrial:test": "yarn fix:tests && yarn test",
    "cleanup": "tsx scripts/project-cleanup.ts",
    "cleanup:report": "yarn paths:check && yarn cleanup && yarn paths:check",
    "integrate": "tsx scripts/integration-cleanup.ts",
    "optimize": "tsx scripts/integration-cleanup.ts",
    "project:init": "powershell -ExecutionPolicy Bypass -File scripts/project-manager.ps1 init",
    "project:validate": "powershell -ExecutionPolicy Bypass -File scripts/project-manager.ps1 validate",
    "project:audit": "tsx scripts/project-layer-auditor.ts",
    "dependency:audit": "tsx scripts/dependency-auditor.ts",
    "modules:check": "tsx scripts/dependency-auditor.ts",
    "paths:check": "tsx scripts/path-validator.ts",
    "paths:validate": "tsx scripts/path-validator.ts",
    "vercel:optimize": "tsx scripts/vercel-optimizer.ts",
    "vercel:ready": "yarn vercel:optimize",
    "vercel:deploy": "vercel --prod",
    "vercel:preview": "vercel",
    "project:purity": "powershell -ExecutionPolicy Bypass -File scripts/project-manager.ps1 purity",
    "project:protect": "powershell -ExecutionPolicy Bypass -File scripts/project-manager.ps1 protect",
    "project:full": "powershell -ExecutionPolicy Bypass -File scripts/project-manager.ps1 full",
    "project:fix-paths": "powershell -ExecutionPolicy Bypass -File scripts/project-manager.ps1 fix-paths",
    "css:check": "tsx scripts/css-check.ts",
    "css:verify": "tsx scripts/css-check.ts",
    "ts:strict": "tsx scripts/ts-strict-enforcer.ts",
    "ts:enforce": "tsx scripts/ts-strict-enforcer.ts",
    "clean": "yarn cache clean && rm -rf .next && rm -rf node_modules/.cache",
    "clean:all": "yarn clean && rm -rf node_modules && yarn install",
    "sw:build": "tsx scripts/build-sw.ts build",
    "sw:watch": "tsx scripts/build-sw.ts watch",
    "sw:dev": "tsx scripts/build-sw.ts dev",
    "deploy": "powershell -ExecutionPolicy Bypass -File scripts/deploy.ps1",
    "deploy:vercel": "powershell -ExecutionPolicy Bypass -File scripts/deploy.ps1 -Target vercel",
    "deploy:docker": "powershell -ExecutionPolicy Bypass -File scripts/deploy.ps1 -Target docker",
    "deploy:compose": "powershell -ExecutionPolicy Bypass -File scripts/deploy.ps1 -Target docker-compose",
    "deploy:git": "powershell -ExecutionPolicy Bypass -File scripts/deploy.ps1 -Target git",
    "docker:build": "docker build -t ultraweb .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker logs euroweb-platform -f",
    "compose:up": "docker-compose up -d",
    "compose:down": "docker-compose down",
    "compose:logs": "docker-compose logs -f",
    "setup:stack": "tsx scripts/setup-stack.ts",
    "fluid:dev": "concurrently \"yarn dev\" \"tsx scripts/fluid-optimizer.ts\"",
    "fluid:boost": "tsx scripts/flow-booster.ts",
    "flow:optimize": "tsx scripts/flow-optimizer.ts",
    "neural:boost": "tsx scripts/neural-booster.ts",
    "system:health": "tsx scripts/system-health.ts",
    "docker:clean": "docker system prune -f",
    "postman:test": "newman run postman/ultraweb-collection.json -e postman/development-environment.json",
    "git:clean": "git gc --aggressive --prune=now",
    "vercel:build": "yarn type-check && next build",
    "vercel:dev": "next dev",
    "vercel:start": "next start",
    "postinstall": "next telemetry disable"
  },
