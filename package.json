{
  "name": "euroweb-platform",
  "version": "8.0.0",
  "description": "EuroWeb - Pure TypeScript Industrial Web8 Platform",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --port 3000",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "lint:app": "eslint --config eslint.config.apponly.ts app/",
    "lint:app:fix": "eslint --config eslint.config.apponly.ts app/ --fix",
    "dev:backend": "tsx backend/server.ts",
    "dev:realtime": "tsx backend/realtime-server.ts",
    "dev:simple": "tsx backend/simple-realtime-server.ts",
    "dev:full": "concurrently \"yarn dev\" \"yarn dev:backend\" \"yarn dev:realtime\"",
    "dev:hybrid": "tsx lib/hybrid/run-hybrid.ts",
    "dev:fluid": "tsx lib/hybrid/run-hybrid.ts",
    "build": "next lint && yarn sw:build && next build",
    "start": "next start",
    "start:hybrid": "NODE_ENV=production tsx lib/hybrid/run-hybrid.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:vitest": "vitest run",
    "test:vitest:watch": "vitest",
    "type-check": "tsc --noEmit",
    "tech:guard": "tsx scripts/tech-guard.ts",
    "pure:check": "tsx scripts/pure-runner.ts check",
    "pure:build": "tsx scripts/pure-runner.ts build",
    "pure:test": "tsx scripts/pure-runner.ts test",
    "pure:dev": "tsx scripts/pure-runner.ts dev",
    "fix:tests": "tsx scripts/test-fixer.ts",
    "fix:routes": "tsx scripts/check-routes.ts --fix",
    "check:purity": "tsx scripts/pure-runner.ts check components/AGISheet/AGIBioNature-pure.tsx",
