    "integrate": "tsx scripts/integration-cleanup.ts",
    "optimize": "tsx scripts/integration-cleanup.ts",
    "project:init": "powershell -ExecutionPolicy Bypass -File scripts/project-manager.ps1 init",
    "project:validate": "powershell -ExecutionPolicy Bypass -File scripts/project-manager.ps1 validate",
    "project:audit": "tsx scripts/project-layer-auditor.ts",
