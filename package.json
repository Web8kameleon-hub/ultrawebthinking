{
  "name": "euroweb-platform",
  "version": "8.0.0",
  "description": "EuroWeb - Pure TypeScript Industrial Web8 Platform",
  "private": true,
  "type": "module",
  "scripts": {
    "dupes": "node --loader ts-node/esm tools/find-duplicates.ts --root ./ --out reports/dupes --threshold=0.90",
    "dupes:ci": "npm run dupes && node --loader ts-node/esm tools/fail-on-duplicates.ts --dup=reports/dupes/duplicates.json --near=reports/dupes/near-duplicates.json --maxExact=0 --maxNear=0",
    "dev": "next dev",
    "dev:backend": "tsx backend/server.ts",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:backend\"",
    "build": "tsc -p tsconfig.mirrors.json && next build",
    "protect": "node dist/scripts/protect-industrial.js",
    "dev:protect": "tsx scripts/protect-industrial.ts",
    "demo:mirror": "node mirror-defense-demo.cjs",
    "demo:build": "npm run dev:protect && npm run demo:mirror",
    "dashboard": "node dashboard/mirror-defense-dashboard.cjs",
    "dashboard:dev": "nodemon dashboard/mirror-defense-dashboard.cjs",
    "build:decoy": "node scripts/build-decoy.js",
    "build:protected": "node scripts/build-protected.js",
    "build:sri": "node scripts/build-sri.js",
    "build:report": "node scripts/build-security-report.js",
    "build:mirrors": "npm run build:decoy && npm run build:protected && npm run build:sri",
    "deploy:industrial": "npm run build:mirrors && npm run build:report",
    "start": "next start",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:system": "tsx test-system-comprehensive.mts",
    "test:mesh": "cd tests/mesh && python test_offline_store_forward.py",
    "test:all": "npm run test && npm run test:system && npm run test:mesh",
    "type-check": "tsc --noEmit",
    "lint": "next lint",
    "lint:check": "eslint . --ext .ts,.tsx --config eslint.config.ts",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:lint": "npm run format && npm run lint",
    "pre-commit": "lint-staged",
    "tech:guard": "tsx scripts/tech-guard.ts",
    "clean": "rimraf dist .next coverage build",
    "clean:all": "npm run clean && rimraf node_modules && npm install",
    "sw:build": "tsx scripts/build-sw.ts build",
    "sw:watch": "tsx scripts/build-sw.ts watch",
    "sw:dev": "tsx scripts/build-sw.ts dev"
  },
