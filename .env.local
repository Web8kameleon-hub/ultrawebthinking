# Development Environment Optimizations
NEXT_TELEMETRY_DISABLED=1
FAST_REFRESH=true

# Disable HMR warnings (Next.js 15 known issue)
__NEXT_DISABLE_WEBCKT_WARNINGS=1

# Build Optimizations
ANALYZE=false
BUNDLE_ANALYZE=false

# Performance
NODE_ENV=development
NODE_OPTIONS="--max-old-space-size=4096"

# AGI System Configuration
AGI_STATUS=OPTIMAL
AGI_PERFORMANCE=100
SEARCH_ENGINE_STATUS=ACTIVE
AI_ANALYSIS_LEVEL=MAXIMUM
LORA_ADAPTATION=ENABLED
WORLD_SURFING=CONNECTED

# Solana UTT-ALB Integration (Faza 0 - Devnet Testing)
SOLANA_NETWORK=devnet
SOLANA_RPC=https://api.devnet.solana.com
ALB_MINT_ADDRESS=HSEcf132J4dNz46gw5fsVV7xfgedeFyTZXMSHcroz3BU
ALB_DECIMALS=6
ALB_EUR_VALUE=100

# Bridge keypair (KURRË mos e commit këtë!)
# Jep NJË nga këto dy:
SOLANA_BRIDGE_KEYPAIR_B58=
SOLANA_BRIDGE_KEYPAIR_PATH=

# Security & Control - DEVNET RELAXED
UTT_MAINNET_TRANSFERS=off
UTT_RATE_LIMIT_PER_DAY=100
UTT_MAX_ALB_PER_TX=1000

# UTT Devnet Configuration
UTT_EURO_PER_ALB=100.0
UTT_TRANSFERS_ENABLED=true
UTT_SHADOW_MODE=false
UTT_PHYSICAL_TOKENS_ENABLED=true
UTT_ALLOWLIST_ENABLED=false
UTT_AUDIT_ENABLED=true
UTT_LOG_LEVEL=debug
