{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "name": "euroweb-platform",
  "alias": [
    "euroweb.ai",
    "www.euroweb.ai",
    "eurowebplatform.com",
    "web8browser.ai",
    "eurowebagi.com",
    "euroweb.com",
    "euroweb.net"
  ],
  "domains": [
    "euroweb.ai",
    "www.euroweb.ai",
    "api.euroweb.ai",
    "admin.euroweb.ai",
    "docs.euroweb.ai",
    "chat.euroweb.ai",
    "ai.euroweb.ai",
    "cdn.euroweb.ai",
    "dev.euroweb.ai",
    "staging.euroweb.ai",
    "eurowebplatform.com",
    "web8browser.ai",
    "eurowebagi.com",
    "euroweb.com",
    "euroweb.net"
  ],
  "build": {
    "env": {
      "DOMAIN": "euroweb.ai",
      "NODE_ENV": "production"
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=()"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/home",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/ai",
      "destination": "/openmind",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/api/ai/:path*",
      "destination": "/api/openmind/:path*"
    }
  ]
}