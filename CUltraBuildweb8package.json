{
  "name": "ultrawebthinking",
  "version": "1.0.0",
  "private": true,
  "packageManager": "yarn@4.9.2",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "rimraf .next node_modules yarn.lock",
    "clean:cache": "rimraf .next node_modules/.cache .yarn/cache",
    "type-check": "tsc --noEmit",
    "test": "",
    "test:ui": " --ui",
    "test:coverage": " --coverage",
    "test:watch": " --watch",
    "backend:install": "cd backend && pip install -r requirements.txt",
    "backend:dev": "cd backend && python start_agi.py",
    "backend:test": "cd backend && python test_agi.py",
    "full:dev": "concurrently \"yarn dev\" \"yarn backend:dev\"",
    "setup": "yarn install && yarn backend:install",
    "docker:build": "docker build -t ultrawebthinking/web8 .",
    "docker:run": "docker run -p 3000:3000 ultrawebthinking/web8",
    "docker:dev": "docker-compose --profile dev up",
    "docker:prod": "docker-compose up",
    "docker:clean": "docker-compose down --volumes --remove-orphans"
  },
  "dependencies": {
    "@fontsource/poppins": "^5.2.6",
    "lowdb": "^7.0.1",
    "lucide-react": "^0.525.0",
    "next": "15.4.1",
    "next-themes": "^0.4.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "tailwindcss": "^4.1.7"
  },
  "devDependencies": {
    "@/uidom": "^10.4.0",
    "@/uireact": "^16.3.0",
    "@/uiuser-event": "^14.6.1",
    "@types/node": "^22.15.18",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "concurrently": "^9.1.0",
    "eslint": "^9.27.0",
    "eslint-config-next": "^15.3.2",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "": "^2.0.0",
    "@/ui": "^2.0.0",
    "jsdom": "^25.0.0"
  },
  "engines": {
    "node": ">=18",
    "yarn": ">=1.22.22"
  },
  "main": "src/index.ts",
  "types": "src/index.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/Web8kameleon-hub/ultrawebthinking.git"
  },
  "author": "Web8kameleon <dealsjona@gmail.com>",
  "license": "MIT"
}
