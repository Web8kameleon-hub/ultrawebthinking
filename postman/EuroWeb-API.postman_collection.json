              "raw": "{{baseUrl}}/api/health",
              "host": ["{{baseUrl}}"],
              "path": ["api", "health"]
            }
          }
        },
        {
          "name": "AGI Status",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/api/agi/status",
              "host": ["{{baseUrl}}"],
              "path": ["api", "agi", "status"]
            }
          }
        },
        {
          "name": "Neural Process",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"query\": \"test neural processing\",\n  \"context\": \"medical\",\n  \"priority\": \"high\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/agi/process",
              "host": ["{{baseUrl}}"],
              "path": ["api", "agi", "process"]
            }
          }
        }
      ]
    },
    {
