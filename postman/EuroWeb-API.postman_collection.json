          "name": "Get Tabs",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/api/tabs",
              "host": ["{{baseUrl}}"],
              "path": ["api", "tabs"]
            }
          }
        },
