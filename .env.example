# UltraWebThinking Web8 Environment Variables

# Application
NEXT_PUBLIC_APP_NAME=UltraWebThinking
NEXT_PUBLIC_APP_VERSION=1.0.0
NEXT_PUBLIC_APP_ENV=development

# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:8000
API_SECRET_KEY=your-secret-key-here

# Database
MONGODB_URI=mongodb://localhost:27017/ultrawebthinking
POSTGRES_URL=postgresql://user:password@localhost:5432/ultrawebthinking

# Authentication
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your-nextauth-secret-here
JWT_SECRET=your-jwt-secret-here

# Third Party Services
OPENAI_API_KEY=your-openai-key-here
ANTHROPIC_API_KEY=your-anthropic-key-here

# Security
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:3001
CORS_ENABLED=true

# AGI Configuration
AGI_MODEL=gpt-4
AGI_MAX_TOKENS=2048
AGI_TEMPERATURE=0.7

# Monitoring
SENTRY_DSN=your-sentry-dsn-here
ANALYTICS_ID=your-analytics-id-here

# Production
REDIS_URL=redis://localhost:6379
DOCKER_REGISTRY=your-registry-here
