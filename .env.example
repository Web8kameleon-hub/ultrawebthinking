# UltraWeb AGI Industrial System - Environment Variables
# Version 8.0.0-WEB8
# Copy this to .env.local for local development

# Application
NODE_ENV=production
NEXT_TELEMETRY_DISABLED=1
SKIP_ENV_VALIDATION=true

# Domain
NEXT_PUBLIC_APP_URL=https://ultraweb.ai

# Brave Search API Configuration
# Get your API key from: https://brave.com/search/api/
BRAVE_SEARCH_API_KEY=your_brave_api_key_here

# Industrial System Configuration
INDUSTRIAL_MODE=true
SYSTEM_MONITORING_ENABLED=true
NEURAL_ENGINE_ENHANCED=true

# API Rate Limits
API_RATE_LIMIT_SEARCH=1000
API_RATE_LIMIT_ANALYSIS=500
API_RATE_LIMIT_REPORTS=100

# Cache Configuration
CACHE_ENABLED=true
CACHE_TTL=1800
REDIS_URL=redis://localhost:6379

# Security Configuration
API_SECRET_KEY=your_secret_key_here
CORS_ENABLED=true
RATE_LIMITING_ENABLED=true

# Neural Engine Configuration
NEURAL_MODEL_CACHE_SIZE=1000
NEURAL_PROCESSING_TIMEOUT=30000
NEURAL_BATCH_SIZE=10

# Industrial Features
ENABLE_PDF_GENERATION=true
ENABLE_SYSTEM_MONITORING=true
ENABLE_REAL_TIME_ANALYTICS=true
ENABLE_BATCH_PROCESSING=true
NEXT_PUBLIC_API_URL=https://api.ultraweb.ai

# Database (if needed)
# DATABASE_URL=postgresql://...

# Redis (if needed)  
# REDIS_URL=redis://...

# Analytics
# VERCEL_ANALYTICS_ID=
# NEXT_PUBLIC_VERCEL_ANALYTICS_ID=

# Security
# JWT_SECRET=your-secret-key
# API_SECRET=your-api-secret

# External APIs
# OPENAI_API_KEY=
# ANTHROPIC_API_KEY=