# UltraWebThinking - Unified Environment Configuration
# Single Port Architecture - No More Port Conflicts!

# ========== PRIMARY CONFIGURATION ==========
NODE_ENV=development
PORT=3000
NEXT_PUBLIC_PORT=3000

# ========== BASE URLS - UNIFIED ==========
NEXT_PUBLIC_BASE_URL=http://localhost:3000
NEXT_PUBLIC_API_URL=http://localhost:3000/api
NEXT_PUBLIC_DOMAIN=ultrawebthinking.com

# ========== AI & AGI MODULE PATHS ==========
AGI_API_PATH=/api/agi
NEURAL_API_PATH=/api/neural
OPENMIND_API_PATH=/api/openmind
CVA_API_PATH=/api/cva

# ========== MEDICAL AI SUITE PATHS ==========
AGIMED_API_PATH=/api/medical/agimed
ALBAMED_API_PATH=/api/medical/albamed
BIONATURE_API_PATH=/api/medical/bionature

# ========== SEARCH & DISCOVERY PATHS ==========
NEURAL_SEARCH_PATH=/api/search/neural
REAL_SEARCH_PATH=/api/search/real
WEB_SEARCH_PATH=/api/search/web

# ========== PERFORMANCE & INFRASTRUCTURE PATHS ==========
ULTRA_INDUSTRIAL_PATH=/api/industrial
LIGHTSPEED_IO_PATH=/api/performance/lightspeed
ZERO_LATENCY_PATH=/api/performance/zero-latency
QUANTUM_PROCESSING_PATH=/api/quantum
NEURAL_ACCELERATION_PATH=/api/performance/neural-acceleration

# ========== SECURITY & GUARDIAN PATHS ==========
GUARDIAN_PATH=/api/security/guardian
ADVANCED_SECURITY_PATH=/api/security/advanced
CYBER_SECURITY_PATH=/api/security/cyber

# ========== NETWORK & COMMUNICATION PATHS ==========
LORA_MESH_PATH=/api/network/lora
IOT_MANAGER_PATH=/api/network/iot
INFINITE_BANDWIDTH_PATH=/api/network/bandwidth

# ========== GAMING & ENTERTAINMENT PATHS ==========
GAMING_AI_PATH=/api/gaming
ENTERTAINMENT_HUB_PATH=/api/entertainment

# ========== SPECIALIZED SYSTEMS PATHS ==========
AVIATION_WEATHER_PATH=/api/specialized/aviation
MARITIME_SYSTEMS_PATH=/api/specialized/maritime
SPACE_TECHNOLOGY_PATH=/api/specialized/space

# ========== DATABASE CONFIGURATION ==========
DATABASE_URL=sqlite:./ultrawebthinking.db
REDIS_URL=redis://localhost:6379

# ========== API KEYS & SECRETS ==========
JWT_SECRET=ultra-web-thinking-secure-key-2024
API_SECRET_KEY=uwt-api-secret-ultra-2024
ENCRYPTION_KEY=uwt-encryption-ultra-secure-2024

# ========== EXTERNAL SERVICES ==========
OPENAI_API_KEY=your-openai-api-key
GOOGLE_AI_API_KEY=your-google-ai-api-key
ANTHROPIC_API_KEY=your-anthropic-api-key

# ========== PERFORMANCE SETTINGS ==========
MAX_CONCURRENT_REQUESTS=1000
CACHE_TTL=3600
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=60

# ========== SECURITY SETTINGS ==========
CORS_ORIGIN=http://localhost:3000
SESSION_TIMEOUT=86400
MAX_FILE_SIZE=10485760
ALLOWED_FILE_TYPES=.jpg,.jpeg,.png,.pdf,.doc,.docx

# ========== LOGGING & MONITORING ==========
LOG_LEVEL=info
LOG_FILE=./logs/ultrawebthinking.log
MONITORING_ENABLED=true
METRICS_ENDPOINT=/api/metrics

# ========== DEPLOYMENT SETTINGS ==========
BUILD_TARGET=standalone
OPTIMIZATION_LEVEL=high
BUNDLE_ANALYZER=false

# ========== LEGACY COMPATIBILITY ==========
# Keep for backward compatibility with existing modules
GATEWAY_INTERNAL_PATH=/api/agi/gateway
ASI_DEMO_INTERNAL_PATH=/api/agi/asi
PRODUCER_INTERNAL_PATH=/api/agi/producer

# ========== MODULE FEATURE FLAGS ==========
ENABLE_AGI_CORE=true
ENABLE_NEURAL_PROCESSING=true
ENABLE_MEDICAL_AI=true
ENABLE_SEARCH_ENGINES=true
ENABLE_SECURITY_MODULES=true
ENABLE_INDUSTRIAL_AUTOMATION=true
ENABLE_NETWORK_MANAGEMENT=true
ENABLE_GAMING_AI=true
ENABLE_SPECIALIZED_SYSTEMS=true

# ========== DEVELOPMENT FLAGS ==========
DEBUG_MODE=true
VERBOSE_LOGGING=true
HOT_RELOAD=true
SOURCE_MAPS=true
