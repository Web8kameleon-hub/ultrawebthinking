{
  "name": "web8",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"yarn dev:frontend\" \"yarn dev:backend\" \"yarn dev:agi\"",
    "dev:frontend": "next dev --port 3000",
    "dev:backend": "tsx backend/server.ts",
    "dev:agi": "tsx backend/agi/core.ts",
    "dev:quick": "next dev --port 3000",
    "dev:debug": "next dev --port 3000 --turbo",
    "dev:inspect": "NODE_OPTIONS='--inspect' next dev --port 3000",
    "dev:verbose": "DEBUG=* next dev --port 3000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "",
    "test:watch": " --watch",
    "test:cov": " --coverage",
    "test:e2e": "playwright test",
    "test:e2e:watch": "playwright test --watch",
    "test:unit": "",
    "test:unit:watch": " --watch",
    "prepare": "husky install",
    "postinstall": "tsc --noemit",
    "debug:routes": "tsx scripts/check-routes.ts",
    "debug:build": "ANALYZE=true yarn build:frontend",
    "check:deps": "yarn why",
    "check:routes": "tsx scripts/check-routes.ts",
    "fix:perms": "chmod +x scripts/*.ts",
    "health": "curl -f http://localhost:3000/api/health || exit 1",
    "push": "tsx scripts/project-control.ts push",
    "kontroll": "tsx scripts/project-control.ts",
    "teste": "tsx scripts/project-control.ts test",
    "deploy": "tsx scripts/project-control.ts deploy",
    "status": "tsx scripts/project-control.ts status"
  },
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.2.0",
    "@mui/material": "^5.2.0",
    "@reduxjs/toolkit": "^1.8.2",
    "apollo-server-micro": "^3.5.0",
    "aws-sdk": "^2.1180.0",
    "bcryptjs": "^2.4.3",
    "concurrently": "^6.2.1",
    "cookie": "^0.4.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "http-proxy-middleware": "^2.0.1",
    "": "^27.2.5",
    "next": "12.0.7",
    "next-compose-plugins": "^2.2.0",
    "next-redux-wrapper": "^7.0.5",
    "nodemailer": "^6.7.2",
    "playwright": "^1.18.0",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "redux-persist": "^6.0.0",
    "sharp": "^0.29.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
