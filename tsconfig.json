{
  "compilerOptions": {
    "target": "ES2022",
    "lib": [
      "dom",
      "dom.iterable",
      "ES6"
    ],
    "allowJs": false,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "@/components/*": ["./components/*"],
      "@/lib/*": ["./lib/*"],
      "@/utils/*": ["./utils/*"],
      "@/types/*": ["./types/*"],
      "@/hooks/*": ["./hooks/*"],
      "@/styles/*": ["./styles/*"],
      "@/public/*": ["./public/*"],
      "@/app/*": ["./app/*"],
      "@/pages/*": ["./pages/*"],
      "@/api/*": ["./app/api/*"],
      "@/config/*": ["./config/*"],
      "@/constants/*": ["./constants/*"],
      "@/contexts/*": ["./contexts/*"],
      "@/providers/*": ["./providers/*"],
      "@/store/*": ["./store/*"],
      "@/data/*": ["./data/*"],
      "@/assets/*": ["./assets/*"],
      "@/tests/*": ["./tests/*"],
      "@/__tests__/*": ["./__tests__/*"],
      "@/scripts/*": ["./scripts/*"],
      "@/middleware": ["./middleware"],
      "@/next.config": ["./next.config"],
      
      // Component-specific aliases
      "@/components/ui/*": ["./components/ui/*"],
      "@/components/layout/*": ["./components/layout/*"],
      "@/components/forms/*": ["./components/forms/*"],
      "@/components/charts/*": ["./components/charts/*"],
      "@/components/tables/*": ["./components/tables/*"],
      "@/components/navigation/*": ["./components/navigation/*"],
      "@/components/modals/*": ["./components/modals/*"],
      "@/components/buttons/*": ["./components/buttons/*"],
      "@/components/inputs/*": ["./components/inputs/*"],
      "@/components/cards/*": ["./components/cards/*"],
      "@/components/animations/*": ["./components/animations/*"],
      "@/components/icons/*": ["./components/icons/*"],
      
      // AGI & EuroWeb specific aliases
      "@/components/agi-office/*": ["./components/agi-office/*"],
      "@/components/AGISheet/*": ["./components/AGISheet/*"],
      "@/components/euroweb/*": ["./components/euroweb/*"],
      "@/components/web8/*": ["./components/web8/*"],
      "@/components/web8-tabs/*": ["./components/web8-tabs/*"],
      "@/components/dashboard/*": ["./components/dashboard/*"],
      "@/components/tabs/*": ["./components/tabs/*"],
      "@/components/excel/*": ["./components/excel/*"],
      "@/components/neural/*": ["./components/neural/*"],
      "@/components/ai/*": ["./components/ai/*"],
      "@/components/automation/*": ["./components/automation/*"],
      "@/components/analytics/*": ["./components/analytics/*"],
      "@/components/reports/*": ["./components/reports/*"],
      "@/components/workflows/*": ["./components/workflows/*"],
      "@/components/integrations/*": ["./components/integrations/*"],
      "@/components/aviation/*": ["./components/aviation/*"],
      
      // New AGI Components aliases
      "@/AGIBioNature": ["./components/AGIBioNature"],
      "@/AGIEco": ["./components/AGIEco"],
      "@/UniversalTranslator": ["./components/UniversalTranslator"],
      "@/EuroMeshDashboard": ["./components/EuroMeshDashboard"],
      "@/UTTDashboard": ["./components/UTTDashboard"],
      "@/UltraQuantumDashboard": ["./components/UltraQuantumDashboard"],
      "@/LoRaPhysicalDashboard": ["./components/LoRaPhysicalDashboard"],
      "@/StationLocationConfig": ["./components/StationLocationConfig"],
      "@/AviationWeatherDashboard": ["./components/aviation/AviationWeatherDashboard"],
      
      // Recent work components
      "@/agi-office": ["./components/agi-office"],
      "@/agi-office/*": ["./components/agi-office/*"],
      "@/agi-office/types": ["./lib/agi-office/types"],
      "@/location": ["./components/LocationConfigDemo"],
      "@/station-config": ["./lib/config/station-location-config"],
      "@/mesh-network": ["./lib/mesh/mesh-networking"],
      "@/loading": ["./components/LoadingSpinner"],
      "@/navigation": ["./lib/navigation"],
      "@/modern-tabs": ["./components/web8-tabs/ModernWeb8TabSystem"],
      
      // Lib-specific aliases
      "@/lib/utils/*": ["./lib/utils/*"],
      "@/lib/api/*": ["./lib/api/*"],
      "@/lib/database/*": ["./lib/database/*"],
      "@/lib/auth/*": ["./lib/auth/*"],
      "@/lib/validation/*": ["./lib/validation/*"],
      "@/lib/helpers/*": ["./lib/helpers/*"],
      "@/lib/hooks/*": ["./lib/hooks/*"],
      "@/lib/services/*": ["./lib/services/*"],
      "@/lib/stores/*": ["./lib/stores/*"],
      "@/lib/config/*": ["./lib/config/*"],
      "@/lib/constants/*": ["./lib/constants/*"],
      "@/lib/types/*": ["./lib/types/*"],
      "@/lib/errors/*": ["./lib/errors/*"],
      "@/lib/middleware/*": ["./lib/middleware/*"],
      "@/lib/security/*": ["./lib/security/*"],
      "@/lib/performance/*": ["./lib/performance/*"],
      "@/lib/analytics/*": ["./lib/analytics/*"],
      "@/lib/monitoring/*": ["./lib/monitoring/*"],
      "@/lib/testing/*": ["./lib/testing/*"],
      "@/lib/deployment/*": ["./lib/deployment/*"],
      "@/lib/optimization/*": ["./lib/optimization/*"],
      "@/lib/i18n/*": ["./lib/i18n/*"],
      
      // Internationalization aliases
      "@/i18n": ["./lib/i18n"],
      "@/i18n/*": ["./lib/i18n/*"],
      "@/routing": ["./lib/i18n/routing"],
      "@/messages": ["./messages"],
      "@/messages/*": ["./messages/*"],
      "@/locales": ["./lib/i18n/locales"],
      "@/locales/*": ["./lib/i18n/locales/*"],
      
      // Our specific lib aliases
      "@/lib/agi-office": ["./lib/agi-office"],
      "@/lib/agi-office/*": ["./lib/agi-office/*"],
      "@/lib/mesh": ["./lib/mesh"],
      "@/lib/mesh/*": ["./lib/mesh/*"],
      
      // UTT-Albion module aliases
      "@/lib/utt": ["./lib/utt"],
      "@/lib/utt/*": ["./lib/utt/*"],
      "@/utt": ["./lib/utt"],
      "@/utt/*": ["./lib/utt/*"],
      "@/albion": ["./lib/utt/albion-token"],
      "@/albion-connection": ["./lib/utt/albion-connection"],
      "@/phantom": ["./lib/utt/phantom-integration"],
      "@/bridge": ["./lib/utt/utt-bridge"],
      "@/physical": ["./lib/utt/utt-physical"],
      "@/audit": ["./lib/utt/utt-audit"],
      "@/gateway": ["./lib/utt/utt-gateway"],
      
      // Feature-specific aliases
      "@/features/*": ["./features/*"],
      "@/features/auth/*": ["./features/auth/*"],
      "@/features/dashboard/*": ["./features/dashboard/*"],
      "@/features/profile/*": ["./features/profile/*"],
      "@/features/settings/*": ["./features/settings/*"],
      "@/features/admin/*": ["./features/admin/*"],
      "@/features/reports/*": ["./features/reports/*"],
      "@/features/analytics/*": ["./features/analytics/*"],
      "@/features/ai/*": ["./features/ai/*"],
      "@/features/automation/*": ["./features/automation/*"],
      "@/features/integrations/*": ["./features/integrations/*"],
      "@/features/workflows/*": ["./features/workflows/*"],
      "@/features/notifications/*": ["./features/notifications/*"],
      "@/features/search/*": ["./features/search/*"],
      "@/features/export/*": ["./features/export/*"],
      "@/features/import/*": ["./features/import/*"],
      
      // Business logic aliases
      "@/business/*": ["./business/*"],
      "@/business/models/*": ["./business/models/*"],
      "@/business/services/*": ["./business/services/*"],
      "@/business/repositories/*": ["./business/repositories/*"],
      "@/business/validators/*": ["./business/validators/*"],
      "@/business/transformers/*": ["./business/transformers/*"],
      "@/business/processors/*": ["./business/processors/*"],
      "@/business/calculators/*": ["./business/calculators/*"],
      "@/business/generators/*": ["./business/generators/*"],
      "@/business/analyzers/*": ["./business/analyzers/*"],
      "@/business/optimizers/*": ["./business/optimizers/*"],
      
      // Infrastructure aliases
      "@/infrastructure/*": ["./infrastructure/*"],
      "@/infrastructure/database/*": ["./infrastructure/database/*"],
      "@/infrastructure/cache/*": ["./infrastructure/cache/*"],
      "@/infrastructure/storage/*": ["./infrastructure/storage/*"],
      "@/infrastructure/queue/*": ["./infrastructure/queue/*"],
      "@/infrastructure/messaging/*": ["./infrastructure/messaging/*"],
      "@/infrastructure/monitoring/*": ["./infrastructure/monitoring/*"],
      "@/infrastructure/logging/*": ["./infrastructure/logging/*"],
      "@/infrastructure/security/*": ["./infrastructure/security/*"],
      "@/infrastructure/networking/*": ["./infrastructure/networking/*"],
      "@/infrastructure/deployment/*": ["./infrastructure/deployment/*"],
      
      // Docker & Deployment aliases
      "@/docker": ["./"],
      "@/docker/*": ["./*"],
      "@/Dockerfile": ["./Dockerfile"],
      "@/Dockerfile.production": ["./Dockerfile.production"],
      "@/docker-compose": ["./docker-compose.yml"],
      "@/docker-compose.dev": ["./docker-compose.dev.yml"],
      "@/deploy": ["./deploy.ps1"],
      "@/deploy-docker": ["./deploy-docker.ps1"],
      "@/health": ["./app/api/health"],
      
      // Scripts & Tools aliases
      "@/tools": ["./tools"],
      "@/tools/*": ["./tools/*"],
      "@/docs": ["./docs"],
      "@/docs/*": ["./docs/*"],
      "@/README": ["./README.md"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "**/*.mts",
    ".next/types/**/*.ts",
    "types/**/*.d.ts",
    "components/**/*.ts",
    "components/**/*.tsx",
    "lib/**/*.ts",
    "lib/**/*.tsx",
    "utils/**/*.ts",
    "hooks/**/*.ts",
    "app/**/*.ts",
    "app/**/*.tsx",
    "pages/**/*.ts",
    "pages/**/*.tsx",
    "features/**/*.ts",
    "features/**/*.tsx",
    "business/**/*.ts",
    "infrastructure/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    ".next",
    "dist",
    "build",
    "coverage",
    "**/*.js",
    "**/*.jsx",
    "**/*.mjs",
    "**/*.cjs",
    "**/*.d.ts.map"
  ]
}
